<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tours</title>
    <link rel="stylesheet" href="/css/style-new.css">
</head>

<body>
    <h1>Hello FSW2</h1>
    <p>
        <%= tours %>
    </p>
    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>nama</th>
                <th>harga</th>
                <th>rating</th>
            </tr>
        </thead>
        <tbody>
            <% tours.map((tour)=> { %>
                <tr>
                    <td>
                        <%= tour.id %>
                    </td>
                    <td>
                        <%= tour.name %>
                    </td>
                    <td>
                        <%= tour.price %>
                    </td>
                    <td>
                        <%= tour.rating %>
                    </td>

                </tr>
                <% }) %>
        </tbody>
    </table>
</body>

</html>